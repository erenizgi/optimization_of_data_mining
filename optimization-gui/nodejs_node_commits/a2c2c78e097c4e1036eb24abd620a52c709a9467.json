            "patch": "@@ -0,0 +1,19 @@\n+'use strict';\n+require('../common');\n+const assert = require('assert').strict;\n+\n+process.env.NODE_DISABLE_COLORS = true;\n+\n+try {\n+  assert.deepStrictEqual({}, { foo: 'bar' });\n+} catch (error) {\n+  const expected =\n+    'Expected values to be strictly deep-equal:\\n' +\n+    '+ actual - expected\\n' +\n+    '\\n' +\n+    '+ {}\\n' +\n+    '- {\\n' +\n+    '-   foo: \\'bar\\'\\n' +\n+    '- }';\n+  assert.strictEqual(error.message, expected);\n+}"
