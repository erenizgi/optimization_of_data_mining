            "contents_url": "https://api.github.com/repos/nodejs/node/contents/tools%2Ficu%2Ficu-generic.gyp?ref=f01adb54542f49af1dc64b36bb9aae17846f6400",
            "patch": "@@ -51,7 +51,7 @@\n       'direct_dependent_settings': {\n         'conditions': [\n           [ 'os_posix == 1 and OS != \"mac\" and OS != \"ios\"', {\n-            'cflags': [ '-Wno-deprecated-declarations' ],\n+            'cflags': [ '-Wno-deprecated-declarations', '-Wno-strict-aliasing' ],\n             'cflags_cc': [ '-frtti' ],\n             'cflags_cc!': [ '-fno-rtti' ],\n           }],"
