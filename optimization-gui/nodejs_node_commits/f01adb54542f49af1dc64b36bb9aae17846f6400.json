            "sha": "b8f0d13836dee176b5fdea348594aaba1f1bac3b",
            "filename": "tools/icu/icu-generic.gyp",
            "status": "modified",
            "additions": 1,
            "deletions": 1,
            "changes": 2,
            "blob_url": "https://github.com/nodejs/node/blob/f01adb54542f49af1dc64b36bb9aae17846f6400/tools%2Ficu%2Ficu-generic.gyp",
