            "patch": "@@ -0,0 +1,12 @@\n+{\n+  \"tests\": [\n+    {\n+      \"cmd\": \"yarn\",\n+      \"args\": [\"tsc\", \"--project\", \"tsconfig.spec.json\", \"--module\", \"commonjs\"]\n+    },\n+    {\n+      \"cmd\": \"yarn\",\n+      \"args\": [\"jasmine\", \"out-tsc/**/*.spec.js\"]\n+    }\n+  ]\n+}"
